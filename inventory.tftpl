[gr_web]
%{ for vm in gr_web }
${vm.name} ansible_host=${vm.network_interface[0].ip_address} fqdn=${vm.fqdn}
%{ endfor }

[gr_db]
%{ for vm in gr_db }
${vm.name} ansible_host=${vm.network_interface[0].ip_address} fqdn=${vm.fqdn}
%{ endfor }

[gr_storage]
%{ for vm in gr_storage }
${vm.name} ansible_host=${vm.network_interface[0].ip_address} fqdn=${vm.fqdn}
%{ endfor }
